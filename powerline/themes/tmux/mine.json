{
    "default_module": "powerline.segments.common",
    "segment_data": {
        "powerline.segments.common.sys.uptime": {
            "before": "⇧  "
        },
        "date": {
            "before": "⌚ "
        },
        "powerline.segments.common.net.hostname": {
            "before": "⌂ "
        }
    },
    "segments": {
        "right": [
            {
                "function": "powerline.segments.common.players.spotify",
                "name": "player"
            },
            {
                "function": "powerline.segments.common.sys.system_load",
                "priority": 50
            },
            {
                "function": "powerline.segments.common.net.internal_ip",
                "args": {
                    "interface": "utun0"
                },
                "before": "utun0 ",
                "priority": 50
            },
            {
                "function": "powerline.segments.common.net.internal_ip",
                "args": {
                    "interface": "en0"
                },
                "before": "en0 ",
                "priority": 50
            },
            {
                "function": "powerline.segments.common.net.internal_ip",
                "args": {
                    "interface": "en1"
                },
                "before": "en1 ",
                "priority": 50
            },
            {
                "function": "powerline.segments.common.net.internal_ip",
                "args": {
                    "interface": "tap0"
                },
                "before": "tap0 ",
                "priority": 50
            },
            {
                "function": "powerline.segments.common.sys.uptime",
                "priority": 50
            },
            {
                "function": "powerline.segments.common.time.date",
                "args": {
                    "format": "%d-%m-%Y",
                    "istime": false
                }
            },
            {
                "function": "powerline.segments.common.time.date",
                "name": "time",
                "args": {
                    "format": "%H:%M",
                    "istime": false
                }
            },
            {
                "function": "powerline.segments.common.net.hostname"
            }
        ]
    }
}
